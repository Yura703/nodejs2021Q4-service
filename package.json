{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "main": "src/server.ts",
  "scripts": {
    "migration:generate": "npm run typeorm migration:generate -- -n MigrationFile",
    "migration:run": "npm run typeorm -- migration:run",
    "generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n migrationFile -p",
    "migration": "ts-node ./node_modules/typeorm/cli -f ./src/ormconfig.ts migration:run",
    "dev": "nodemon ./src/server.ts",
    "start": "npm run migration && npm run dev",
    "start:generate": "npm run generate && migration && npm run dev",
    "build": "tsc",
    "lint": "eslint --ext .js,.ts --fix src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@types/node": "^16.11.11",
    "bcrypt": "^5.0.1",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "fastify": "^3.24.1",
    "fastify-swagger": "^4.12.6",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "pino": "^7.6.1",
    "query-string": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.4.0",
    "typeorm": "^0.2.41",
    "typescript": "^4.5.2",
    "uuid": "8.3.2",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pino": "^7.0.5",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "chai": "4.3.4",
    "debug": "4.3.2",
    "dirty-chai": "2.0.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "husky": "^4.3.8",
    "jest": "27.3.1",
    "lint-staged": "12.0.3",
    "nodemon": "2.0.15",
    "parse-url-query-params": "0.0.6",
    "prettier": "2.4.1",
    "supertest": "6.1.6"
  }
}
